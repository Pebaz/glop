let x = 0,
let y = 0,

loop
[
    if @u64-gte(y, 64)
    [
        break,
    ]
    else
    [
        loop
        [
            if @u64-gte(x, 64)
            [
                break,
            ]
            else
            [
                @draw-pixel(
                    x,
                    y,
                    @u64-add(150, x),
                    @u64-add(150, y),
                    @u64-add(50, @u64-add(x, y)),
                ),
            ]

            set x = @u64-add(x, 1),
        ]
    ]

    set y = @u64-end(y, 1),
]
